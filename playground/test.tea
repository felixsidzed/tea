using "io";

class Test
	public var a: int;
	private var b: int;

	Test(int x)
		a = x;
		b = 41;
	end

	public func compute() -> int
		return a - b;
	end
end

public func main()
	var ts: Test = Test(67);
	io::printf("result = %d\n", ts.compute());
	var x: func(int)(Test*) = ts.compute;

	return 0;
end
